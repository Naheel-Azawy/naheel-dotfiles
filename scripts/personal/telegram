#!/bin/sh

ICON= # defined below
AGENT='Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/101.0.4951.26 Safari/537.36'

run() {
    exec chromium --user-agent="$AGENT" --app=https://web.telegram.org/z
}

mk() {
    user_desktop="$HOME/.local/share/applications/telegram.desktop"
    user_icon="$HOME/.local/share/pixmaps/telegram.png"

    [ -f "$user_icon" ] || {
        mkdir -p "$(dirname "$user_icon")" &&
            echo "$ICON" | base64 -d > "$user_icon"
    }

    [ -f "$user_desktop" ] || {
        me=$(realpath "$0")
        mkdir -p "$(dirname "$user_desktop")" && {
            echo '[Desktop Entry]'
            echo "Type=Application"
            echo "Name=Telegram"
            echo "Icon=$user_icon"
            echo "Exec=$me"
            echo "Categories=Web;Messaging"
        } > "$user_desktop"
    }
}

main() {
    mk && run
}

